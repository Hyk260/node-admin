{
  "name": "node-admin",
  "version": "1.0.0",
  "private": true,
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "node app.js",
    "prod": "set NODE_ENV=production && nodemon app.js",
    "app": "nodemon app.js",
    "lint": "eslint \"**/*.{js,ts}\"",
    "pkg": "pkg . --output=bin/node-admin",
    "pkgwin": "pkg . -t node16-win-x64 -o app",
    "pkgmacos": "pkg . -t node16-macos-x64 -o app"
  },
  "dependencies": {
    "@vercel/node": "^2.8.4",
    "axios": "^1.1.3",
    "body-parser": "^1.20.1",
    "debug": "^4.3.4",
    "eslint": "^7.32.0 || ^8.2.0",
    "express-fileupload": "^1.4.0",
    "express-jwt": "^6.0.0",
    "express-session": "^1.17.3",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "lowdb": "^1.0.0",
    "morgan": "^1.10.0",
    "node-fetch": "^3.2.10",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.1",
    "progress-stream": "^2.0.0",
    "screenshot-desktop": "^1.12.7",
    "tls-sig-api-v2": "^1.0.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.2",
    "express": "^4.17.1"
  },
  "bin": "./app.js",
  "pkg": {
    "assets": [],
    "scripts": [],
    "targets": [
      "node16-macos-x64",
      "node16-win-x64"
    ],
    "outputPath": "dist"
  },
  "author": "",
  "license": "ISC",
  "description": ""
}
